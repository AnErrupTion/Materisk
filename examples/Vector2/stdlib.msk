mod pub Console
{
	fn native pub print(str text) void {}

	fn pub println(str text) void
	{
		Console.print(text);
		Console.print("\n");
	}
}

fn native pub allocate(i32 size) ptr void {}
fn native pub free(ptr void pointer) void {}

fn pub lenof(str text) u32
{
	var index mut u32 = 0;
	while (text[index++] != (u8)0) {};
	return index;
}