using "stdlib.msk";

mod pub dyn Vector2
{
	fld pub x int;
	fld pub y int;

	fn pub ctor(int x, int y) void
	{
		self.x = x;
		self.y = y;
	}

	fn pub print() void
	{
		Console.print(str "<Vector2 x=");
		Console.print(str Int.toString(self.x));
		Console.print(str " y=");
		Console.print(str Int.toString(self.y));
		Console.println(str ">");
	}

	fn pub add(Vector2 other) Vector2
	{
		return new Vector2(self.x + other.x, self.y + other.y);
	}
}