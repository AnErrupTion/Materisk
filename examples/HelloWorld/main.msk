using "corelib.msk";

mod dyn Test
{
	fld pub x u32;

	fn pub ctor(ptr Test self) void
	{
		self.x = 9;
	}
}

mod pub Program
{
	fn pub main() void
	{
		Console.println("Hello, World!");

		var test ptr Test = alloc Test();
		var x u32 = test.x;

		if (x == 9)
		{
			Console.println("Pass!");
		}
	}
}